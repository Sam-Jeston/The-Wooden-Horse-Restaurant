<!-- Navigation -->
<head>
  <title>Wooden Horse Restaurant & Bar</title>
</head>
<span itemscope itemtype="http://schema.org/Restaurant">
<div class="container">
<div class="row">
  <nav class="navbar navbar-default" role="navigation">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <!-- navbar-brand is hidden on larger screens, but visible when the menu is collapsed -->
      <a class="navbar-brand" href>Wooden Horse</a>
    </div>
    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="hidden-xs">
      <%=image_tag 'logoblackontransparent.png', class: "center-block", style: "opacity: 0.65; width:40%; height: 40%;"%>
        <hr style="margin-top:0px; margin-bottom:5px">
    </div>
    <div class="collapse navbar-collapse" id="navbar-collapse">
      <ul class="nav navbar-nav">
        <li>
          <%=link_to "Home", root_path, style: "padding:12px" %>
        </li>
        <li>
          <%=link_to "About", about_path, style: "padding:12px" %>
        </li>
        <li>
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" style="padding:12px" aria-haspopup="true" aria-expanded="false">Menus <span class="caret"></span></a>
          <ul class="dropdown-menu">
            <li>
              <%=link_to "Food", menu_path, style: "padding:12px", itemprop: "menu" %>
            </li>
            <li>
              <%=link_to "Drink", drink_path, style: "padding:12px" %>
            </li>
          </ul>
        </li>
        <li>
          <%=link_to "Functions", functions_path, style: "padding:12px" %>
        </li>
        <li>
          <%=link_to "Contact", new_contact_path, style: "padding:12px" %>
        </li>
        <li>
          <%=link_to "Gallery", gallery_path, style: "padding:12px" %>
        </li>
      </ul>
    </div>
    <!-- /.navbar-collapse -->
  <!-- /.container -->
  </nav>
</div>
</div>
